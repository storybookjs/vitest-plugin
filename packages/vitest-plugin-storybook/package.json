{
	"name": "vitest-plugin-storybook",
	"version": "1.0.0",
	"description": "Vite plugin for story testing powered by Vitest",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"vite",
		"plugin",
		"storybook",
		"test",
		"testing",
		"vitest"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs --dts",
		"check": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/storybookjs/FILL_ME_HERE.git"
	},
	"author": "Yann Braga <yannbf@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/storybookjs/FILL_ME_HERE/issues"
	},
	"homepage": "https://github.com/storybookjs/FILL_ME_HERE#readme",
	"peerDependencies": {
		"vitest": ">=0.29.0"
	},
	"devDependencies": {
		"@storybook/react": "^8.1.0-beta.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@types/node": "^20.12.11",
		"@types/testing-library__jest-dom": "^6.0.0",
		"jsdom": "^24.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"ts-morph": "^22.0.0",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.1",
		"typescript": "^5.4.5",
		"vite": "^5.0.2",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"magic-string": "^0.30.10",
		"typescript": "^5.0.0"
	}
}